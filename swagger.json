        ],
        "responses": {
          "200": {
            "description": "A list of games",
            "content": {
              "application/json": {
                "schema": {
                  "$schema": "https://json-schema.org/draft/2020-12/schema",
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "const": true
                    },
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "universeId": {
                            "type": "number"
                          },
                          "rootPlaceId": {
                            "type": "number"
                          },
                          "name": {
                            "type": "string"
                          },
                          "description": {
                            "anyOf": [
                              {
                                "type": "string"
                              },
                              {
                                "type": "null"
                              }
                            ]
                          },
                          "gameplayDescription": {
                            "anyOf": [
                              {
                                "type": "string"
                              },
                              {
                                "type": "null"
                              }
                            ]
                          }
                        },
                        "required": [
                          "universeId",
                          "rootPlaceId",
                          "name",
                          "description",
                          "gameplayDescription"
                        ],
                        "additionalProperties": false
                      }
                    }
                  },
                  "required": [
                    "success",
                    "data"
                  ],
                  "additionalProperties": false
                },
                "example": {
                  "success": true,
                  "data": [
                    {
                      "universeId": 0,
                      "rootPlaceId": 0,
                      "name": "string",
                      "description": null,
                      "gameplayDescription": null
                    }
                  ]
                }
              }
            }
          },
          "default": {
            "description": "Global error response",
            "content": {
              "application/json": {
                "example": {
                  "success": false,
                  "message": "Error message"
                },
                "schema": {
