    "/games": {
      "get": {
        "summary": "Get the list of all games in the database.",
        "operationId": "getGames",
        "parameters": [
          {
            "name": "limit",
            "in": "query",
            "description": "Maximum number of games to return",
            "required": false,
            "schema": {
              "type": "integer",
              "minimum": 1
